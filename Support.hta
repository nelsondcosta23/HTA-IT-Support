                    "Descreva da melhor forma o seu problema, caso necessário anexe imagens." & vbCrLf & _
                    "Obrigado" & vbCrLf & vbCrLf & _
					"Problema:"
            .Display
        End With
        On Error GoTo 0
        Set OutMail = Nothing
        Set OutApp = Nothing	
	End Sub
